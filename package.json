{
  "name": "web3insight",
  "private": true,
  "sideEffects": false,
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@9.4.0",
  "scripts": {
    "env": "node .knosys/scripts env",
    "build": "next build --webpack",
    "build:turbo": "next build",
    "predev": "npm run env",
    "dev": "next dev --turbopack",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint . --ext .js,.jsx,.ts,.tsx",
    "start": "next start",
    "typecheck": "tsc --noEmit || true",
    "prepare": "husky || exit 0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --ignore-unknown --ignore-path ./.gitignore --write",
      "eslint --cache --fix"
    ],
    "*.{ts,tsx}": [
      "prettier --ignore-unknown --ignore-path ./.gitignore --write",
      "eslint --cache --fix"
    ],
    "*.yml": "eslint --cache --fix"
  },
  "dependencies": {
    "@ai-sdk/azure": "^0.0.17",
    "@ai-sdk/openai": "^0.0.40",
    "@highcharts/map-collection": "^2.3.2",
    "@nextui-org/react": "^2.6.11",
    "@privy-io/react-auth": "^3.8.0",
    "@t3-oss/env-nextjs": "0.13.8",
    "@tanstack/react-query": "^5.84.2",
    "ai": "^3.3.6",
    "clsx": "^2.1.1",
    "dayjs": "1.10.8",
    "echarts": "5.5.1",
    "echarts-for-react": "^3.0.2",
    "framer-motion": "^12.7.4",
    "highcharts": "^12.4.0",
    "highcharts-react-official": "^3.2.3",
    "jose": "^5.10.0",
    "jotai": "^2.9.2",
    "lucide-react": "^0.417.0",
    "next": "^16.0.7",
    "next-themes": "^0.4.6",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-markdown": "^9.0.1",
    "react-responsive": "^10.0.0",
    "recharts": "^3.1.2",
    "server-only": "^0.0.1",
    "tailwind-merge": "^3.2.0",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@knosys/sdk": "^0.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.7",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "9.1.7",
    "ksio": "^0.0.3",
    "lint-staged": "15.3.0",
    "postcss": "^8.4.41",
    "semver": "7.6.3",
    "tailwindcss": "^3.4.9",
    "typescript": "^5.5.4"
  }
}